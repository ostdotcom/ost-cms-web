<div class="app-content h-100">
<%= render :partial => "/web/ost/index/sidebar" %>
<nav>
  <ul class="nav nav-tabs justify-content-end">
    <li class="nav-item">
      <a class="nav-link active nav-item" id="nav-home-tab" data-toggle="tab" href="#nav-draft" role="tab" aria-controls="nav-draft" aria-selected="false">Draft Mode</a>
    </li>
    <li class="nav-item">
      <a class="nav-item nav-link" id="nav-profile-tab" data-toggle="tab" href="#nav-published" role="tab" aria-controls="nav-published" aria-selected="false">Published</a>
    </li>
    <li class="nav-item pr-3">
      <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Actions</a>
      <div class="dropdown-menu dropdown-menu-right">
        <a class="dropdown-item j-create-link" href="#" data-toggle="modal" data-target="#genericModal">Create New</a>
        <div class="dropdown-divider"></div>
        <a class="dropdown-item j-reset-publish-link" href="#">Reset to Published Version</a>
        <a class="dropdown-item j-publish-changes-link" href="#">Publish Changes</a>
      </div>
    </li>
  </ul>
</nav>
<div class="content-wrapper ">
  <div class="container-fluid">
    <div class="tab-content pb-4" id="nav-tabContent">

      <div class="tab-pane fade show active mt-4" id="nav-draft" role="tabpanel" aria-labelledby="nav-home-tab">
        <div id="list"></div>
      </div>

      <div class="modal fade" id="genericModal" tabindex="-1" role="dialog" aria-labelledby="genericModalLabel" aria-hidden="false">
        <div class="modal-dialog modal-lg" role="document">
          <div class="modal-content">
          </div>
        </div>
      </div>


      <div class="modal fade" id="" tabindex="-1" role="dialog" aria-labelledby="genericModalLabel" aria-hidden="false">
        <div class="modal-dialog modal-lg" role="document">
          <div class="modal-content">
          </div>
        </div>
      </div>




      <div class="modal" id="deleteModal" data-backdrop="static">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class=" modal-body text-center py-4">
              <div class="confirm_message text-size-normal">Are you sure you want to delete this item?</div>
              <button class="btn btn-primary mt-3 delete-btn">Proceed</button>
              <button class="btn btn-secondary mt-3" data-dismiss="modal">Cancel</button>
            </div>
          </div>
        </div>
      </div>


      <div class="tab-pane fade mt-4" id="nav-published" role="tabpanel" aria-labelledby="nav-profile-tab">
       <div id="published_list"></div>
      </div>
    </div>

  </div>


</div>
<!-- /.container-fluid -->
</div>

<% content_for :end_js do %>
  <%= render :partial => "/shared/ost_form_builder/all_partials_templates" %>
  <script type="text/javascript">
    cms.entity.init({
      'entityConfig' : <%= raw @config_response.to_json %>
    });

  </script>
<% end %>

<!-- /.content-wrapper -->
<!-- /#wrapper -->